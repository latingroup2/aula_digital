<div class="page-header">
  <h3>Try #<%= @try.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @try.user_id %></dd>

      <dt>Excercise</dt>
      <dd><%= @try.excercise_id %></dd>

      <dt>User answer</dt>
      <dd><%= @try.user_answer %></dd>

      <dt>Correct</dt>
      <dd><%= @try.correct %></dd>

      <dt>Active</dt>
      <dd><%= @try.active %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/tries" class="btn btn-primary">
        Back
      </a>
      <a href="/tries/<%= @try.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_try/<%= @try.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
