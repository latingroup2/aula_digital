<div class="page-header">
  <h1>
    Objetivos
    <a href="/objetivos/new" class="btn btn-lg btn-success">New Objetivo</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Pre objetivo</th>
        <th>Next objetivo</th>
        <th>Eje</th>
        <th>Description</th>
        <th>Nivel</th>
        <th>Actions</th>
      </tr>

      <% @objetivos.each do |objetivo| %>
      <tr>
        <td><%= objetivo.pre_objetivo_id %></td>
        <td><%= objetivo.next_objetivo_id %></td>
        <td>
          <% if objetivo.eje.present? %>
            <a href="/ejes/<%= objetivo.eje.id %>">
              <%= objetivo.eje.description %>
            </a>
          <% end %>
        </td>
        <td><%= objetivo.description %></td>
        <td>
          <% if objetivo.year.present? %>
            <a href="/nivels/<%= objetivo.year.id %>">
              <%= objetivo.year.intended_year %>
            </a>
          <% end %>
        </td>
        <td>
          <a href="/objetivos/<%= objetivo.id %>" class="btn btn-primary">Show</a>
          <a href="/objetivos/<%= objetivo.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_objetivo/<%= objetivo.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @objetivos, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
