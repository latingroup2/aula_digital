<div class="page-header">
  <h1>
    Ejes
    <a href="/ejes/new" class="btn btn-lg btn-success">New Eje</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Materia</th>
        <th>Description</th>
        <th>Actions</th>
      </tr>

      <% @ejes.each do |eje| %>
      <tr>
        <td><%= eje.materia_id %></td>
        <td><%= eje.description %></td>
        <td>
          <a href="/ejes/<%= eje.id %>" class="btn btn-primary">Show</a>
          <a href="/ejes/<%= eje.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_eje/<%= eje.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
