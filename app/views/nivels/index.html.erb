<div class="page-header">
  <h1>
    Nivels
    <a href="/nivels/new" class="btn btn-lg btn-success">New Nivel</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Intended year</th>
        <th>Actions</th>
      </tr>

      <% @nivels.each do |nivel| %>
      <tr>
        <td><%= nivel.intended_year %></td>
        <td>
          <a href="/nivels/<%= nivel.id %>" class="btn btn-primary">Show</a>
          <a href="/nivels/<%= nivel.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_nivel/<%= nivel.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @nivels, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
